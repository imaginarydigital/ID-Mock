{{!-- view container --}}
<div class="animate-view-container">
  <ng-view class="animate-view" view-animation></ng-view>
</div>


<div class="container-fluid">
  <div class="row">
    <div class="col-sm-12 contact-col" style="background-color: gray">
    <section class="contact">
      <h2>Contact</h2>
      <div class="content">
        <a href="" class="btn btn-primary btn-lg" data-toggle="modal" data-target=".bs-example-modal-lg">Send Us A Message</a>
        <ul class="list-inline">
        <li><a href=""><i class="fa fa-5x fa-facebook-official"></i></a></li>
        <li><a href=""><i class="fa fa-5x fa-instagram"></i></a></li>
        <li><a href=""><i class="fa fa-5x fa-youtube-square"></i></a></li>
        <li><a href=""><i class="fa fa-5x fa-envelope-o"></i></a></li>
      </ul>
      </div>
    </section>
    </div>
  </div>
</div>
  


  {{!-- modal --}}
    <div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title">Contact Form</h4>
          </div>
          <div class="modal-body">
            <p>Fill out the form bitch boy&hellip;</p>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            <button type="button" class="btn btn-primary">Save changes</button>
          </div>
        </div><!-- /.modal-content -->
      </div><!-- /.modal-dialog -->
    </div>
  {{!-- /.modal --}}



    <script type="text/javascript" src="js/vendor.js"></script>
    <script type="text/javascript" src="js/main.js"></script>
</body>
</html>